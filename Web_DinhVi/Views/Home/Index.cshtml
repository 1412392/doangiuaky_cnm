
@{
    ViewBag.Title = "Định Vị Xe - Trang Chủ";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div  id="floating-panel">
    <input id="address" type="text" value="512 cách mạng tháng 8">


    <input type="button" value="Encode" onclick="codeAddress($('#address').val())">
    <br>

    <select v-model="type" class="form-control" id="selection">
        <option value="1" selected>Khách chưa xử lý</option>
    </select>
    <input type="button" value="Xác định" onclick="submitAddress()">
</div>


